{
  "name": "graphiql-with-explorer",
  "version": "3.1.0",
  "license": "MIT",
  "description": "A GraphiQL docker app",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "scripts": {
    "build:esm": "esbuild src/index.jsx --bundle --outfile=dist/index.js --format=esm --define:process.env.NODE_ENV=\\\"production\\\" --define:process.env.NODE_DEBUG=false --define:global=window --main-fields=module,browser,main",
    "build:cjs": "esbuild src/index.jsx --bundle --outfile=dist/index.cjs.js --format=cjs --define:process.env.NODE_ENV=\\\"production\\\" --define:process.env.NODE_DEBUG=false --define:global=window --main-fields=module,browser,main",
    "build:css": "node build.css.js && esbuild src/discovery.css --bundle --outfile=dist/discovery.css",
    "build": "npm run build:esm && npm run build:cjs && npm run build:css",
    "start": "npm run build && node server.js",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@discoveryjs/discovery": "^1.0.0-beta.57",
    "@types/react": "^17.0.2",
    "assert": "^2.0.0",
    "css-tree": "^1.1.2",
    "dset": "^3.1.0",
    "ejs": "^3.0.1",
    "esbuild": "^0.8.42",
    "express": "^4.17.1",
    "graphiql": "1.4.0-rc.1",
    "graphiql-explorer": "^0.6.2",
    "graphql": "14.5.8",
    "punycode": "^2.1.1",
    "react": "16.12.0",
    "react-dom": "^17.0.1"
  }
}
