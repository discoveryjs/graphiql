{
  "name": "@discoveryjs/graphiql",
  "version": "1.4.1",
  "license": "MIT",
  "description": "A GraphiQL docker app",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build:bundle": "esbuild --bundle --minify --outdir=dist --define:process.env.NODE_ENV=\\\"production\\\" --define:process.env.NODE_DEBUG=false --define:global=window --main-fields=module,browser,main",
    "build:js": "npm run build:bundle -- src/index.tsx src/create-discovery-instance.js --format=esm",
    "build:css": "npm run build:bundle -- src/index.css src/create-discovery-instance.css --log-level=error && node scripts/cleanup-css-hacks",
    "build": "npm run build:js && npm run build:css",
    "start": "npm run build && node scripts/server.js",
    "dev": "mkdir -p dev && node scripts/gen-html > dev/index.html && npm run build:bundle -- src/index.tsx src/create-discovery-instance.js src/index.css src/create-discovery-instance.css --outdir=dev --servedir=dev --format=esm --sourcemap",
    "lint": "eslint src scripts",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@discoveryjs/discovery": "^1.0.0-beta.59",
    "@types/codemirror": "0.0.108",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "assert": "^2.0.0",
    "css-tree": "^1.1.2",
    "dset": "^3.1.0",
    "ejs": "^3.0.1",
    "esbuild": "^0.9.0",
    "eslint": "^7.22.0",
    "express": "^4.17.1",
    "graphiql": "1.4.7",
    "graphiql-explorer": "^0.6.2",
    "graphql": "14.5.8",
    "punycode": "^2.1.1",
    "react": "16.12.0",
    "react-dom": "^17.0.1",
    "typescript": "^4.2.3"
  }
}
